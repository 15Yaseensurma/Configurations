{
    "id": "11111111",
    "title": "Salesforce",
    "version": "1.0.0",
    "icon": "https://docs.secureauth.com/download/attachments/44830637/Salesforce.png",
    "aliases": [
        "SalesforceSP"
    ],
    "entries":
    {
        "RedirectType":
        {
            "label": "Authenticated User Redirect",
            "controlType": "select",
            "defaultValue": "Saml2IdpInitiated",
            "width": 5,
            "options": [
            {
                "value": "Saml2SpInitiated",
                "display": "SAML 2.0 (SP Initiated) Assertion"
            },
            {
                "value": "Saml2IdpInitiated",
                "display": "SAML 2.0 (SP Initiated) Assertion"
            },
            {
                "value": "Saml2SpInitiatedByPost",
                "display": "SAML 2.0 (SP Initiated) Assertion"
            }]
        },
        "WsFedReplyTo_SamlTargetUrl":
        {
            "label": "SAML Target URL",
            "visible": false,
            "controlType": "input",
            "defaultValue": "https://saml.salesforce.com"
        },
        "SAMLAudience":
        {
            "label": "SAML Audience",
            "visible": false,
            "controlType": "input",
            "defaultValue": "https://saml.salesforce.com"
        },
        "xxxConsumerID":
        {
            "label": "SAML Consumer URL ID",
            "controlType": "input",
            "extras": "xxxConsumerIDTooltip",
            "spacer": "\xa0\xa0",
            "width": 5
        },
        "Issuer":
        {
            "label": "SAML Issuer",
            "controlType": "input",
            "description": "A unique name that must match exactly on the SecureAuth IdP side and the Salesforce side",
            "width": 5
        },
        "SAMLValidHours":
        {
            "label": "SAML Valid Hours",
            "controlType": "input",
            "type": "number",
            "defaultValue": "24",
            "description": "How long SAML assertion is valid",
            "width": 2
        },
        "SAMLConsumerURL":
        {
            "label": "SAML Consumer URL",
            "visible": false,
            "controlType": "input",
            "fixedValue": "https://login.salesforce.com?so=%{_.xxxConsumerID}"
        },
        "SAMLRecipient":
        {
            "label": "SAML Recipient URL",
            "visible": false,
            "controlType": "input",
            "fixedValue": "https://login.salesforce.com?so=%{_.xxxConsumerID}"
        }
    },
    "statics":
    {
        "xxxConsumerIDTooltip":
        {
            "content": "Customer's Salesforce ID number (can be found in the Salesforce admin console, in the Endpoints section of the Security Controls --> Single Sign-On Settings)"
        }
    },
    "order": [
        "RedirectType",
        "WsFedReplyTo_SamlTargetUrl",
        "SAMLAudience",
        "xxxConsumerID",
        "Issuer",
        "SAMLValidHours",
        "SAMLConsumerURL",
        "SAMLRecipient"
    ]
}

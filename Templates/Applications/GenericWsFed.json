{
    "id": "22222222",
    "title": "Generic WS-Fed",
    "version": "1.0.0",
    "icon": "https://docs.secureauth.com/download/attachments/44830637/UserExchange.png",
    "aliases": [
        "WSFed",
        "WS-Fed",
        "Default WS Fed"
    ],
    "entries":
    {
        "RedirectType":
        {
            "label": "Authenticated User Redirect",
            "visible": false,
            "controlType": "select",
            "defaultValue": "WsFederation",
            "options": [
            {
                "value": "WsFederation",
                "display": "WS-Federation Assertion"
            }]
        },
        "WsFedReplyTo_SamlTargetUrl":
        {
            "label": "WS-Federation ReplyTo URL",
            "controlType": "input",
            "width": 5
        },
        "AppendHttpsToTargetUrl":
        {
            "visible": false,
            "controlType": "switch",
            "defaultValue": true,
            "description": "Append HTTPS to WS-Federation ReplyTo URL (if not included)."
        },
        "GenerateUniqueAssertionId":
        {
            "visible": false,
            "controlType": "switch",
            "defaultValue": false,
            "description": "Generate GUID to pass to Service Provider"
        },
        "WsFedVersion":
        {
            "label": "WS-Federation Version",
            "controlType": "select",
            "defaultValue": "1.3",
            "width": 2,
            "options": [
            {
                "value": "1.2",
                "display": "1.2"
            },
            {
                "value": "1.3",
                "display": "1.3"
            }]
        },
        "WsFedSigningAlgorithm":
        {
            "label": "WS-Federation Signing Algorithm",
            "controlType": "select",
            "defaultValue": "SHA2",
            "description": "Algorithm used to sign WS-Federation assertion",
            "width": 2,
            "options": [
            {
                "value": "SHA1",
                "display": "SHA-1"
            },
            {
                "value": "SHA2",
                "display": "SHA-2"
            }]
        },
        "Issuer":
        {
            "label": "WS-Federation Issuer",
            "controlType": "input",
            "description": "A unique name that must match exactly on the SecureAuth IdP side and the WS-Fed side",
            "width": 5
        },
        "EndpointConfiguration":
        {
            "label": "Endpoint Configuration",
            "controlType": "input",
            "description": "Settings for WS-Trust endpoints",
            "width": 5
        },
        "Endpoints":
        {
            "label": "Endpoint Settings",
            "controlType": "input",
            "width": 5
        },
        "Host":
        {
            "label": "SecureAuth Host",
            "controlType": "input",
            "description": "Base DNS address of public SecureAuth IdP URL",
            "width": 5
        },
        "Id":
        {
            "label": "WS-Trust ID",
            "controlType": "input",
            "description": "WS-Trust endpoint name",
            "width": 5
        },
        "Enabled":
        {
            "label": "Enable Endpoint",
            "controlType": "switch",
            "defaultValue": false
        },
        "EndpointPath":
        {
            "label": "WS-Trust Endpoint Path",
            "controlType": "input",
            "width": 5
        },
        "AuthenticationType":
        {
            "label": "Authentication Type",
            "controlType": "input",
            "description": "Type of authentication supported",
            "width": 5
        },
        "SecurityMode":
        {
            "label": "Security Mode",
            "controlType": "input",
            "width": 5
        },
        "Type":
        {
            "label": "WS-Trust Type",
            "controlType": "input",
            "width": 5
        }
    },
    "order": [
        "RedirectType",
        "WsFedVersion",
        "WsFedReplyTo_SamlTargetUrl",
        "AppendHttpsToTargetUrl",
        "Issuer",
        "GenerateUniqueAssertionId",
        "WsFedSigningAlgorithm",
        "EndpointConfiguration",
        "Endpoints",
        "Host",
        "Id",
        "Enabled",
        "EndpointPath",
        "AuthenticationType",
        "SecurityMode",
        "Type"
    ]
}
